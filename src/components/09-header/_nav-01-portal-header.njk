{% macro portal_header(portal_header) %}
  <div class="portal-header">
    <div class="portal-header-inner">
      <div class="usa-logo" id="{{ portal_header.id_prefix }}logo">
        <em class="usa-logo-text">
          <a href="{{ portal_header.link.href | default('/') }}"
            title="{{ portal_header.link.title | default('Home') }}"
            aria-label="{{ portal_header.link.label | default('Home') }}">
            {{ portal_header.title | default(site.title) }}
          </a>
        </em>
      </div>
    </div>
  </div>
{% endmacro %}